@prefix vgw: <https://vangoghworldwide.org/data/concept/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

vgw:f_number a skos:Concept ;
    skos:prefLabel "De La Faille number" ;
    skos:scopeNote "Catalogue number used in The Works of Vincent van Gogh: His Paintings and Drawings by Jacob Baart de la Faille." .

vgw:jh_number a skos:Concept ;
    skos:prefLabel "JH number" ;
    skos:scopeNote "Catalogue number used in The Complete Van Gogh: Paintings, Drawings, Sketches by Jan Hulsker." .

vgw:previous_attribution a skos:Concept ;
    skos:prefLabel "Previously attributed to Vincent van Gogh" ;
    skos:scopeNote "Concept to indicate for a work that Vincent van Gogh was previously attributed as it's contributor but evindence has shown that this was not the case." .

vgw:questionable_attribution a skos:Concept ;
    skos:prefLabel "Questionable attribution to Vincent van Gogh" ;
    skos:scopeNote "Concept to indicate for a work that Vincent van Gogh is questioned as it's contributor." .

vgw:empty_value a skos:Concept ;
    skos:prefLabel "No value" ;
    skos:scopeNote "Concept to indicate that a work misses a value in facet. This is used in Spinque facet strategies. It is not intended for use directly in the data." .

vgw:identifier a rdf:Property ;
    rdfs:label "VGW Identifier" ;
    rdfs:comment "Identifier used in VGW for artworks" .
